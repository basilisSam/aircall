{
  "name": "aicall",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aircall/tractor": "^1.16.1",
    "prop-types": "^15.8.1",
    "pusher-js": "^7.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-paginate": "^8.1.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-toastify": "^8.1.1",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "build": "react-scripts build",
    "test:watch": "test --watchAll=true",
    "test": "react-scripts test --watchAll=false --transformIgnorePatterns \"node_modules/(?!@toolz/allow-react)/\"",
    "test:coverage": "react-scripts test --coverage --watchAll=false --transformIgnorePatterns \"node_modules/(?!@toolz/allow-react)/\"",
    "build:prod": "npm run test && npm run lint && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@jest/globals": "^26.0.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "eslint": "^8.9.0",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2"
  }
}
